{
    "customBaseDir" :"/var/lib/cloud9/artGateway/upLoadInfos/",
    "customDefaultAtt" :{
        "type": "file",
        "absolute": true,
        "alwaysIncludePattern": true
    },
    "appenders": [
        {
            "type": "console","category": "console"
        },
        {
            "filename": "deviceAttris/deviceAttris.info",
            "pattern": "_yyyy-MM-dd",
            "maxLogSize": 2048,
            "backups": 3,
            "category": "deviceAttris"
        },
        {
          "filename": "deviceStatus/deviceStatus.info",
          "pattern": "_yyyy-MM-dd",
          "maxLogSize": 524288,
          "backups": 10,
          "category": "deviceStatus"
        }
    ],
    "replaceConsole": true,
    "levels":{ "deviceAttris": "INFO", "deviceStatus": "INFO"}
}